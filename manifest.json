{
	"name": "Catagolue Reloaded",
	"version": "3.9",
	"manifest_version": 2,

	"description": "Useful enhancements for Adam P. Goucher's Catagolue",
	"developer": {
		"name": "Apple Bottom",
		"url": "https://twitter.com/_AppleBottom_"
	},
	"icons": {
		"128": "catagolue2.0_128.png",
		"48": "catagolue2.0_48.png",
		"16": "catagolue2.0_16.png"
	},
	"web_accessible_resources": [
		"images/closeButton.png",
		"scripts/comments.js",
		"scripts/dragondrop.js",
		"scripts/md5.js",
		"scripts/sampleSoupOverlay.js"
	],

	"content_scripts": [
		{
			"matches": ["*://catagolue.appspot.com/*"],
			"include_globs": ["*://catagolue.appspot.com/object/*", "*://catagolue.appspot.com/object?*"],
			"js": ["catagoluereloaded.js"]
		},
		{
			"matches": ["*://catagolue.appspot.com/*"],
			"include_globs": ["*://catagolue.appspot.com/census/*"],
			"js": ["symmetryinjector.js"]
		},
		{
			"matches": ["*://catagolue.appspot.com/*"],
			"include_globs": ["*://catagolue.appspot.com/census", "*://catagolue.appspot.com/census/"],
			"js": ["maincensuspage.js"]
		}

	]
}